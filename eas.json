{
  "cli": {
    "version": ">= 11.0.0"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "simulator": false,
        "autoIncrement": "buildNumber"
      }
    },
    "production": {
      "channel": "production",
      "ios": {
        "simulator": false,
        "autoIncrement": "buildNumber"
      }
    }
  }
}
