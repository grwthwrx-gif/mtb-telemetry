{
  "expo": {
    "name": "mtb-telemetry",
    "slug": "mtb-telemetry",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/icons/psynk/icon_square_1024.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/psynk/splash_dark_2048.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0C10",
      "dark": {
        "image": "./assets/images/psynk/splash_dark_2048.png",
        "resizeMode": "contain",
        "backgroundColor": "#0B0C10"
      },
      "light": {
        "image": "./assets/images/psynk/splash_dark_2048.png",
        "resizeMode": "contain",
        "backgroundColor": "#F0F0F0"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "4"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/psynk/icon_square_512.png",
        "backgroundColor": "#0B0C10"
      },
      "versionCode": 4
    },
    "web": {
      "favicon": "./assets/icons/psynk/icon_square_512.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/psynk/Orbitron-Regular.ttf",
            "./assets/fonts/psynk/Orbitron-Bold.ttf"
          ]
        }
      ]
    ]
  }
}