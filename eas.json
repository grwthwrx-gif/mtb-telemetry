{
  "cli": {
    "version": ">= 11.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "distribution": "app-store",
      "ios": {
        "simulator": false,
        "autoIncrement": true
      },
      "android": {
        "gradleCommand": ":app:bundleRelease",
        "autoIncrement": true
      }
    }
  },
  "submit": {
    "production": {
      "ios": {}
    }
  }
}
